                    <!-- feed 텍스트 영역 -->
                    <div class="feed-list-text">
                        <div style="font-size: 40px;"><%=result[i].profess_title%></div>
                        <div><%=result[i].profess_content%></div>
                    </div>
                    <!--// feed 텍스트 영역 -->
                 <!-- /feed list 바디 -->
             </div>
             <!--// feed list 영역 -->
             <%}%>
         <%}%>
